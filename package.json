  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3stacks/es6-module-boilerplate/issues"
  },
  "homepage": "https://github.com/3stacks/es6-module-boilerplate#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015-native-modules": "^6.9.2",
    "babel-preset-es2017": "^6.14.0",
    "webpack": "^2.1.0-beta.21"
  },
  "dependencies": {
    "babel-runtime": "^6.9.2"
